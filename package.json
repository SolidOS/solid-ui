{
  "name": "solid-ui",
  "version": "2.3.8",
  "description": "UI library for writing Solid read-write-web applications",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "files": [
    "lib",
    "dist"
  ],
  "scripts": {
    "build": "npm run clean && npm run build-version && npm run build-lib && npm run build-types && npm run build-webpack # FIXME: && npm run build-storybook",
    "build-lib": "babel src -d lib --source-maps --extensions \".ts,.js\"",
    "build-types": "tsc --emitDeclarationOnly",
    "build-version": "sh ./timestamp.sh > src/versionInfo.ts  && eslint \"src/versionInfo.ts\" --fix",
    "build-webpack": "webpack",
    "watch": "npm run build-version && babel src --out-dir lib --watch --source-maps --extensions \".ts,.js\"",
    "clean": "rm -rf lib",
    "coverage": "jest --coverage --collectCoverageFrom=src/**/*.[jt]s",
    "lint": "eslint \"src/**/*.js\" \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint-fix": "eslint \"src/**/*.js\" \"src/**/*.ts\" --fix",
    "jest": "jest",
    "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand --watch",
    "test": "npm run build && npm run lint && npm run jest",
    "doc": "typedoc --out ./Documentation/api/ ./src/ --stripInternal",
    "prepublishOnly": "npm test && npm run build",
    "postpublish": "git push origin master --follow-tags",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook  --output-dir ./examples/storybook"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/solid/solid-ui"
  },
  "keywords": [
    "solid",
    "decentralized",
    "widgets",
    "ui",
    "web",
    "rdf",
    "ldp",
    "linked",
    "data"
  ],
  "author": "Tim Berners-Lee <timbl@mit.edu>",
  "contributors": [
    "Daniel Friedman <danielf@mit.edu>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/solid/solid-ui/issues"
  },
  "homepage": "https://github.com/solid/solid-ui",
  "dependencies": {
    "@babel/runtime": "^7.12.5",
    "@inrupt/solid-client-authn-browser": "^0.3.0",
    "crypto-browserify": "^3.12.0",
    "escape-html": "^1.0.3",
    "jss": "^10.5.0",
    "jss-preset-default": "^10.5.0",
    "mime-types": "^2.1.27",
    "node-uuid": "^1.4.7",
    "pane-registry": "^2.3.4",
    "path-browserify": "^1.0.1",
    "rdflib": "^2.1.3",
    "solid-auth-client": "^2.5.4",
    "solid-logic": "^1.2.8",
    "solid-namespace": "^0.5.0",
    "stream-browserify": "^3.0.0",
    "uuid": "^8.3.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.8",
    "@babel/core": "^7.12.9",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-optional-chaining": "^7.12.7",
    "@babel/plugin-transform-async-to-generator": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.7",
    "@babel/preset-typescript": "^7.12.7",
    "@storybook/addon-actions": "^6.1.9",
    "@storybook/addon-essentials": "^6.1.9",
    "@storybook/addon-links": "^6.1.9",
    "@storybook/html": "^6.1.9",
    "@types/jest": "^26.0.15",
    "@types/jsdom": "^16.2.5",
    "@types/node": "^14.14.10",
    "@typescript-eslint/eslint-plugin": "^4.8.2",
    "@typescript-eslint/parser": "^4.8.2",
    "babel-loader": "^8.2.2",
    "eslint": "^7.14.0",
    "eslint-plugin-jest": "^24.1.3",
    "husky": "^4.3.0",
    "jest": "^26.6.3",
    "jsdom": "^16.4.0",
    "lint-staged": "^10.5.2",
    "react-is": "^17.0.1",
    "solid-auth-cli": "^1.0.15",
    "standard": "^16.0.3",
    "ts-jest": "^26.4.4",
    "typedoc": "^0.19.2",
    "typescript": "^4.1.2",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm test"
    }
  },
  "lint-staged": {
    "src/**/*.(js|ts)": [
      "eslint"
    ]
  }
}
